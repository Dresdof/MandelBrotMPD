global Mandelbrot
	
	type Complex = ptr ComplexRec;

	type ComplexRec = rec(
		real realPart;
		real imaginaryPart;
	);

	op complex(real realPart, real imaginaryPart) returns Complex c

	op mandelbrotIterate(Complex complex, int maxIterations) returns int i
	
	op absolute(Complex complex) returns real r
	
	op assignateComplexArray() returns Complex

body Mandelbrot separate
